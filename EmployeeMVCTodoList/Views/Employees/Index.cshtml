@model List<EmployeeMVCTodoList.Models.Domain.Employee>

@{
}

<h1>Employees</h1>

<table class="table">
	<thead>
		<tr>
			<td>Id</td>
			<td>Name</td>
			<td>Email</td>
			<td>Salary</td>
			<td>Date of Birth</td>
			<td>Department</td>
		</tr>
	</thead>
	<tbody>
		@foreach (var employee in Model)
		{
			<tr>
				<td>@employee.Id</td>
				<td>@employee.Name</td>
				<td>@employee.Email</td>
				<td>@employee.Salary</td>
				<td>@employee.DateOfBirth.ToString("dd-MM-yyy")</td>
				<td>@employee.Department</td>
			</tr>
		}
	</tbody>
</table>










